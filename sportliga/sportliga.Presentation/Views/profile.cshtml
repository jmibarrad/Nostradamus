<link href="~/Views/profile.css" rel="stylesheet" type="text/css" />    
            
                 
<div class="container">
    <div class="row clearfix">
        <div class="col-md-12 column">
            <ul class="nav nav-tabs">
                <li class="active">
                    <a href="#">Profile</a>
                </li>
                <li>
                    <a href="./login">Home</a>
                </li>
                <li class="disabled">
                    <a href="#">Messages</a>
                </li>
                <li class="dropdown pull-right">
                    <a href="#" data-toggle="dropdown" class="dropdown-toggle">Options<strong class="caret"></strong></a>
                    <ul class="dropdown-menu">
                        <li>
                            <a href="#">Action</a>
                        </li>
                        <li>
                            <a href="#">Another action</a>
                        </li>
                        <li>
                            <a href="#">Something else here</a>
                        </li>
                        <li class="divider">
                        </li>
                        <li>
                            <a href="#">Separated link</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>


    
    <div class="row clearfix">
        <div class="col-md-12 column">
            <div class="user-image">
                <img src="data:image/jpeg;base64," width="150" height="120" alt="Jimmy Ramos" title="Jimmy Ramos" class="img-circle">
            </div>
            <div class="user-info-block">
                <div class="user-heading">
                    <h3>Yuomas Holopainen</h3>
                    <span class="help-block">Helsinky, Finland</span>
                </div>
                <ul class="navigation">
                    <li class="active">
                        <a data-toggle="tab" href="#information">
                            <span class="glyphicon glyphicon-user"></span>
                        </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#settings">
                            <span class="glyphicon glyphicon-cog"></span>
                        </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#email">
                            <span class="glyphicon glyphicon-envelope"></span>
                        </a>
                    </li>
                    <li>
                        <a data-toggle="tab" href="#events">
                            <span class="glyphicon glyphicon-calendar"></span>
                        </a>
                    </li>
                </ul>
                <div class="user-body">
                    <div class="tab-content">
                        <div id="information" class="tab-pane active">
                            <h4>Account Information</h4>
                        </div>
                        <div id="settings" class="tab-pane">
                            <h4>Settings</h4>
                        </div>
                        <div id="email" class="tab-pane">
                            <h4>Send Message</h4>
                        </div>
                        <div id="events" class="tab-pane">
                            <h4>Events</h4>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

   

    <div class="row clearfix">
        <div class="col-md-12 column">
            <div>
                <div class="panel-heading">
                    <div class="input-group">
                        <span class="input-group-addon"><i class="glyphicon glyphicon-search"></i></span>
                        <input type="text" class="form-control" placeholder="Search League" ng-model="buscar.nombre">
                    </div>
                </div>
                <div class="btn-toolbar">
                    <button class="btn btn-primary" ng-click="activate()">New League</button>
                </div>
                <div class="panel-body">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>
                                    <a href="" ng-click="ordenarPor('nombre')">
                                        Team Name(s)
                                    </a>
                                    <span class="caret" style="cursor: pointer" ng-click="ordenarPor('-nombre')"></span>
                                </th>
                                <th>Country</th>
                                <th>Start Date</th>
                                <th>End Date</th>
                                <th>Total Teams</th>
                                <th>Active</th>
                                <th style="width: 47px;"></th>                     
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="liga in ArrLeagues | orderBy:OrdenSeleccionado | filter:buscar">
                                <td><a class="btn-link" href="/league/{{liga.id}}">{{liga.nombre}}</a></td>
                                <td>{{liga.pais | uppercase}}</td>
                                <td>{{liga.seasonStart | date:'fullDate'}}</td>
                                <td>{{liga.seasonEnd | date:'fullDate'}} </td>
                                <td>{{liga.numberOfTeams}}</td>
                                <td>{{liga.active}}</td>
                                <td>
                                    <a style="color: red" href="user.html"><i class="glyphicon glyphicon-flag"></i></a>
                                    <a style="color: yellowgreen" href="user.html"><i class="glyphicon glyphicon-pencil"></i></a>
                                    <a style="color: grey" href="#myModal" role="button" data-toggle="modal"><i class="glyphicon glyphicon-trash"></i></a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row" ng-show="adding">
    <form class="col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4" ng-submit="addNewLeague()">
        <p style=" font-family: times, Times New Roman, times-roman, georgia, serif; color: #444; margin: 0; padding: 0px 0px 6px 0px; color:darkgoldenrod; font-size: 51px; line-height: 44px; padding-left: 10em; letter-spacing: -2px; font-weight: bold">Add New League</p><br />
        <div class="form-box">
            <div class="form-group">
                <input ng-model="_leagueName" type="text" class="form-control" placeholder="League´s Name" required />
                <input ng-model="_country" type="text" class="form-control" placeholder="Country" required />
                <input ng-model="__leagueID" type="text" class="form-control" placeholder="ID" required />
            </div>
            <div class="form-group">
                <button class="btn btn-warning btn-block" type="submit">Save</button>
                <button class="btn btn-danger btn-block" type="button" ng-click="IsFinishedAddTeam()">Cancel</button>
            </div>
        </div>
    </form>
</div>

<div class="modal small hide fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3 id="myModalLabel">Delete Confirmation</h3>
    </div>
    <div class="modal-body">
        <p class="error-text">Are you sure you want to delete the user?</p>
    </div>
    <div class="modal-footer">
        <button class="btn" data-dismiss="modal" aria-hidden="true">Cancel</button>
        <button class="btn btn-danger" data-dismiss="modal">Delete</button>
    </div>
</div>

